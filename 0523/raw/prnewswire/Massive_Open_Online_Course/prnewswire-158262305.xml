  &lt;p itemprop="articleBody"&gt;
   &lt;span class="xn-location" itemprop="contentLocation" itemscope="" itemtype="http://schema.org/Place"&gt;
    &lt;span itemprop="geo" itemscope="" itemtype="http://schema.org/address"&gt;
